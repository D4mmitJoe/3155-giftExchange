<h1>Create a New Room</h1>
<p>Must rename view as newroom</p>
<%= form_for :room, url: rooms_path do |f| %>
  <p>
    <%= f.label :roomcode %><br>
    <%= f.text_field :roomcode %> <!-- change back to password only allow Admin to see this field-->
  </p>
 
  <p>
    <%= f.label :roompassword %><br>
    <%= f.text_field :roompassword %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>