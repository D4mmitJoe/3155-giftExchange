<h1>Create Account</h1>

<%= form_for :welcome, url: welcome_path do |f| %>

    <p>
        <%= f.label :fname %><br>
        <%= f.text_field :fname %>
    </p>
    
    <p>
        <%= f.label :lname %><br>
        <%= f.text_area :lname %>
    </p>
    
    <p>
        <%= f.submit %>
    </p>
    
<% end %>